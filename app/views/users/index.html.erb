<ul>
  <% @users.each do |user| %>
    <li><%= user %></li>
  <% end %>
</ul>
<%= link_to "Search User", search_users_path(@invoice), method: :post %>
<h4>Search By user</h4>
<% text_field_tag(:search, params[:search])%>
<%= submit_tag ("Search") %>

<%= form_for User.new, :url => "search" do |f| %>
  <%= f.label :email %>
  <%- f.text_field :email %>

<%- end %>
